<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.czl2">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:name="com.example.czl2.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Czl">
        <activity android:name="com.example.czl2.privacy.PrivacyActivity"></activity>
        <activity android:name="com.example.czl2.component.Service2Activity" />
        <activity android:name="com.example.czl2.component.Service1Activity" />

        <service
            android:name="com.example.czl2.component.Service1"
            android:enabled="true"
            android:exported="true" />

        <activity android:name="com.example.czl2.component.Fragment1Activity" />
        <activity android:name="com.example.czl2.component.Activity2" />
        <activity android:name="com.example.czl2.component.Activity1" />
        <activity android:name="com.example.czl2.RVActivity" />
        <activity android:name="com.example.czl2.SecondActivity" />
        <activity android:name="com.example.czl2.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>